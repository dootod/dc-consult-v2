<form method="post" action="{{ path('app_delete_mes_documents', {'id': document.id}) }}" onsubmit="return confirm('Êtes-vous sûr de vouloir supprimer ce document ?');">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ document.id) }}">
    <button type="submit">Supprimer</button>
</form>